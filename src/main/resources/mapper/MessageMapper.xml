<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.mysite.practice.message.MessageMapper">
	<select id="getMessageById" resultType="com.mysite.practice.message.MessageDTO">
		select id, message from hello where id = #{id};
	</select>
	<select id="getMessageListAll" resultType="com.mysite.practice.message.MessageDTO">
		select * from hello;
	</select>
</mapper>